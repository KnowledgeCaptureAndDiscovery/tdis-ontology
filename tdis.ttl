@base <https://w3id.org/metadata/tdis> .

<https://w3id.org/metadata/tdis> rdf:type owl:Ontology ;
                                  owl:imports :tdis_boundary_types ,
                                              :tdis_organizations .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/metadata/tdis#hasCategory
:hasCategory rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#hasContact
:hasContact rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :ModelConfiguration
                                        :Artifact
                                        :Collection
                                        :Layer
                                        :Model
                                      )
                        ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#hasDistributorOrPublisher
:hasDistributorOrPublisher rdf:type owl:ObjectProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :ModelConfiguration
                                                       :Feature
                                                       :Layer
                                                       :Model
                                                     )
                                       ] ;
                           rdfs:range [ rdf:type owl:Class ;
                                        owl:unionOf ( :Distributor
                                                      :Publisher
                                                    )
                                      ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#hasServiceEndpoint
:hasServiceEndpoint rdf:type owl:ObjectProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :ModelConfiguration
                                                :Feature
                                                :Layer
                                                :Model
                                              )
                                ] ;
                    rdfs:range :ServiceEndpoint .


